{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "yarn build:api && yarn build:docs",
    "build:api": "tsc -p tsconfig.build.json",
    "build:docs": "compodoc -t -d ../docs/api -p tsconfig.json",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "prestart:prod": "rimraf api && npm run build",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:debug": "nodemon --config nodemon-debug.json",
    "start:dev": "concurrently --handle-input \"wait-on api/main.js && nodemon\" \"tsc -w -p tsconfig.build.json\" ",
    "start:prod": "node api/main.js",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@nestjs/common": "6.10.1",
    "@nestjs/core": "6.10.1",
    "@nestjs/jwt": "6.1.1",
    "@nestjs/mongoose": "6.1.2",
    "@nestjs/passport": "6.1.0",
    "@nestjs/platform-express": "6.10.1",
    "@nestjs/serve-static": "2.1.3",
    "@sentry/node": "5.12.4",
    "chalk": "3.0.0",
    "dotenv": "8.2.0",
    "firebase-admin": "8.8.0",
    "flatten-obj": "3.1.1",
    "mailgun-js": "0.22.0",
    "moment": "2.24.0",
    "mongoose": "5.7.12",
    "morgan": "1.9.1",
    "nest-morgan": "1.0.1",
    "nest-raven": "5.0.0",
    "passport": "0.4.0",
    "passport-google-oauth20": "2.0.0",
    "passport-jwt": "4.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.0",
    "rxjs": "6.5.3",
    "sharp": "0.24.0",
    "uuid-v4": "0.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.7.2",
    "@compodoc/compodoc": "1.1.11",
    "@nestjs/testing": "6.10.1",
    "@types/express": "4.17.2",
    "@types/jest": "24.0.23",
    "@types/morgan": "1.7.37",
    "@types/node": "12.12.11",
    "@types/passport": "1.0.2",
    "@types/supertest": "2.0.8",
    "babel-loader": "8.0.6",
    "concurrently": "5.0.0",
    "jest": "24.9.0",
    "nodemon": "2.0.0",
    "supertest": "4.0.2",
    "ts-jest": "24.1.0",
    "ts-node": "8.5.2",
    "tsconfig-paths": "3.9.0",
    "tslint": "5.20.1",
    "tslint-config-airbnb": "5.11.2",
    "typescript": "3.7.2",
    "wait-on": "3.3.0",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.10",
    "webpack-node-externals": "1.7.2"
  },
  "engines": {
    "yarn": "^1.19.1",
    "node": "12.13.0"
  }
}
